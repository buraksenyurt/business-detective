<configuration>
	<configSections>
		<section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net" />
	</configSections>
	<log4net>
		<appender name="RollingFileAppender" type="log4net.Appender.RollingFileAppender">
			<lockingModel type="log4net.Appender.FileAppender+MinimalLock" />
			<encoding value="utf-8" />
			<file value="logs/sys" />
			<datePattern value="_yyyyMMddHH.'log'" />
			<staticLogFileName value="false" />
			<appendToFile value="true" />
			<rollingStyle value="Date" />
			<maxSizeRollBackups value="100" />
			<maximumFileSize value="1MB" />
			<layout type="log4net.Layout.PatternLayout">
				<conversionPattern value="%-5p%d{ yyyy-MM-dd HH:mm:ss} [%thread](%logger) %m %exception %n%n" />
			</layout>
		</appender>
		<root>
			<level value="ALL" />
			<appender-ref ref="RollingFileAppender" />
		</root>
	</log4net>
	<appSettings>
		<add key="SolutionPath" value="C:\Users\burak\source\repos\business-detective\src\BusinessDetective\BusinessDetective.sln" />
		<add key="BusinessProjectName" value="BusinessLibrary" />
		<add key="BusinessContractProjectName" value="BusinessContracts" />
		<add key="log4net.Config" value="log4.config"/>
		<add key="log4net.Config.Watch" value="True"/>
		<add key="log4net.Internal.Debug" value="False"/>
		<add key="EndpointsFileName" value="Endpoints.xml"/>
	</appSettings>
